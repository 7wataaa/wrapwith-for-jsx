{
  "name": "wrapwith-for-jsx",
  "displayName": "wrapWith for JSX",
  "description": "Extension to enclose a JSX-tag with a new JSX-tag",
  "repository": "https://github.com/7wataaa/wrapwith-for-jsx",
  "image": "images/wrapwith-for-jsx-icon.png",
  "license": "MIT",
  "publisher": "7wataaa",
  "keywords": [
    "jsx",
    "tsx",
    "snippet"
  ],
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wrapwith-for-jsx.wrapWith",
        "title": "wrapwith-for-jsx: wrapWith"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/types": "^7.18.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "vsce": "^2.9.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/parser": "^7.18.5",
    "@babel/traverse": "^7.18.5"
  }
}